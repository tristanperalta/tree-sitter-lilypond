{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "lilypond",
  "rules": {
    "lilypond": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_top_level_expression"
      }
    },
    "_top_level_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "version_statement"
        },
        {
          "type": "SYMBOL",
          "name": "header_block"
        },
        {
          "type": "SYMBOL",
          "name": "book_block"
        },
        {
          "type": "SYMBOL",
          "name": "bookpart_block"
        },
        {
          "type": "SYMBOL",
          "name": "score_block"
        },
        {
          "type": "SYMBOL",
          "name": "paper_block"
        },
        {
          "type": "SYMBOL",
          "name": "layout_block"
        },
        {
          "type": "SYMBOL",
          "name": "midi_block"
        },
        {
          "type": "SYMBOL",
          "name": "assignment"
        },
        {
          "type": "SYMBOL",
          "name": "embedded_scm"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line_comment"
        },
        {
          "type": "SYMBOL",
          "name": "block_comment"
        }
      ]
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "%"
          },
          {
            "type": "PATTERN",
            "value": "[^{\\n][^\\n]*"
          }
        ]
      }
    },
    "block_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "%{"
          },
          {
            "type": "PATTERN",
            "value": "[^%]*(%+[^%}][^%]*)*"
          },
          {
            "type": "STRING",
            "value": "%}"
          }
        ]
      }
    },
    "version_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\version"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          },
          "named": true,
          "value": "version_number"
        }
      ]
    },
    "header_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\header"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_header_body"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_header_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "assignment"
        },
        {
          "type": "SYMBOL",
          "name": "embedded_scm"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "book_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\book"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_book_body"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_book_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_block"
        },
        {
          "type": "SYMBOL",
          "name": "bookpart_block"
        },
        {
          "type": "SYMBOL",
          "name": "score_block"
        },
        {
          "type": "SYMBOL",
          "name": "paper_block"
        },
        {
          "type": "SYMBOL",
          "name": "embedded_scm"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "bookpart_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\bookpart"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_bookpart_body"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_bookpart_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_block"
        },
        {
          "type": "SYMBOL",
          "name": "score_block"
        },
        {
          "type": "SYMBOL",
          "name": "paper_block"
        },
        {
          "type": "SYMBOL",
          "name": "embedded_scm"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "score_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\score"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_score_body"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_score_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_block"
        },
        {
          "type": "SYMBOL",
          "name": "layout_block"
        },
        {
          "type": "SYMBOL",
          "name": "midi_block"
        },
        {
          "type": "SYMBOL",
          "name": "embedded_scm"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "paper_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\paper"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_output_body"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "layout_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\layout"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_output_body"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "midi_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\midi"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_output_body"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_output_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "assignment"
        },
        {
          "type": "SYMBOL",
          "name": "embedded_scm"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "assignment_id"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "_identifier_init"
        }
      ]
    },
    "assignment_id": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "symbol"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "_identifier_init": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number_expression"
        },
        {
          "type": "SYMBOL",
          "name": "embedded_scm"
        }
      ]
    },
    "number_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_LEFT",
          "value": 1,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "number_expression"
              },
              {
                "type": "STRING",
                "value": "+"
              },
              {
                "type": "SYMBOL",
                "name": "number_term"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 1,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "number_expression"
              },
              {
                "type": "STRING",
                "value": "-"
              },
              {
                "type": "SYMBOL",
                "name": "number_term"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "number_term"
        }
      ]
    },
    "number_term": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_LEFT",
          "value": 2,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "number_term"
              },
              {
                "type": "STRING",
                "value": "*"
              },
              {
                "type": "SYMBOL",
                "name": "number_factor"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 2,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "number_term"
              },
              {
                "type": "STRING",
                "value": "/"
              },
              {
                "type": "SYMBOL",
                "name": "number_factor"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "number_factor"
        }
      ]
    },
    "number_factor": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 3,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "-"
              },
              {
                "type": "SYMBOL",
                "name": "number_factor"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "bare_number"
        }
      ]
    },
    "bare_number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "unsigned"
        },
        {
          "type": "SYMBOL",
          "name": "real"
        },
        {
          "type": "SYMBOL",
          "name": "fraction"
        }
      ]
    },
    "unsigned": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "real": {
      "type": "PATTERN",
      "value": "[0-9]*\\.[0-9]+"
    },
    "fraction": {
      "type": "PATTERN",
      "value": "[0-9]+\\/[0-9]+"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"([^\"\\\\]|\\\\.)*\""
    },
    "symbol": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_-]*"
    },
    "embedded_scm": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "scheme_boolean"
            },
            {
              "type": "SYMBOL",
              "name": "scheme_number"
            },
            {
              "type": "SYMBOL",
              "name": "scheme_symbol"
            }
          ]
        }
      ]
    },
    "scheme_boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "##t"
        },
        {
          "type": "STRING",
          "value": "##f"
        },
        {
          "type": "STRING",
          "value": "#t"
        },
        {
          "type": "STRING",
          "value": "#f"
        }
      ]
    },
    "scheme_number": {
      "type": "SYMBOL",
      "name": "unsigned"
    },
    "scheme_symbol": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}